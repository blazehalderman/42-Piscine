#include <stdio.h>

void	ft_swap(int *a, int *b)
{
	int temp;

	temp = *a;
	*a = *b;
	*b = temp;
}


int main()
{
	int x;
	int y;
	int b;
	b = 0;
	x = 5;
	y = 1;
	b = ft_swap(&x,&y);
	printf("%d", b);
	return(0);
}
